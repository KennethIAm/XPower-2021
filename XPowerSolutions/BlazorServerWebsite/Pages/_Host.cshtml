@page "/"
@namespace BlazorServerWebsite.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using BlazorServerWebsite.Data.Models
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>BlazorServerWebsite</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/libs/bootstrap/css/bootstrap.css" />
    <link href="css/site.css" rel="stylesheet" />
    <link href="BlazorServerWebsite.styles.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css">
</head>
<body>
    <component type="typeof(App)" render-mode="Server"  />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_framework/blazor.server.js"></script>
    <script src="~/css/libs/bootstrap/js/bootstrap.bundle.js"></script>
    <script src="~/js/site.js"></script>
</body>
</html>
